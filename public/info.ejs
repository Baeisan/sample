        <script>
          function apply(){
            axios.post('/applyMiro', {
              name: document.getElementById('userName').value,
              major: document.getElementById('userMajor').value,
              studentnumber: document.getElementById('userStudentnumber').value,
              grade: document.getElementById('userGrade').value,
              motive: document.getElementById('userMotive').value,
              phonenumber: document.getElementById('userPhonenumber').value
            })
            .then(function(res){
              document.getElementById('userName').value = "";
              document.getElementById('userMajor').value = "";
              document.getElementById('userStudentnumber').value = "";
              document.getElementById('userGrade').value = "";
              document.getElementById('userMotive').value = "꼭 작성하셔야합니다. 선별여부에 영향있음.";
